<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ROOT xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<HEADER>
    <TYPE>
        <EXTENSION_TABLE_NAME>SI_MODULE_IV_TEST</EXTENSION_TABLE_NAME>
        <NAME>SI MODULE IV TEST DATA</NAME>
    </TYPE>
    <RUN>
        <RUN_TYPE>{{run_type}}</RUN_TYPE>
        <RUN_NUMBER>{{run_number}}</RUN_NUMBER>
        <RUN_BEGIN_TIMESTAMP>{{run_begin_timestamp}}</RUN_BEGIN_TIMESTAMP>
        <RUN_END_TIMESTAMP>{{run_end_timestamp}}</RUN_END_TIMESTAMP>
        <INITIATED_BY_USER>{{initiated_by_user }}</INITIATED_BY_USER>
        <COMMENT_DESCRIPTION>{{ comment_description }}</COMMENT_DESCRIPTION>
    </RUN>
</HEADER>
    <DATA_SET>
        <COMMENT_DESCRIPTION>SI Module Plots Data</COMMENT_DESCRIPTION>
        <VERSION>1</VERSION>
        <PART mode="auto">
            <KIND_OF_PART>HAD 300um Si Module LD Full</KIND_OF_PART>
            <SERIAL_NUMBER>{{serial_number}}</SERIAL_NUMBER>
        </PART>
        <DATA>
            <REL_HUM>{{ rel_hum }}</REL_HUM>
            <TEMP_C>{{ temp_c }}</TEMP_C>
            <STATUS>{{ status }}</STATUS>
            <STATUS_DESC>{{ status_desc }}</STATUS_DESC>
            <GRADE>{{ grade }}</GRADE>
            <RATIO_I_AT_VS>{{ ratio_i_at_vs }}</RATIO_I_AT_VS>
            <RATIO_AT_VS>{{ ratio_at_vs }}</RATIO_AT_VS>
            <INSPECTOR>{{ inspector }}</INSPECTOR>
            <PROGRAM_V>{{ program_v }}</PROGRAM_V>
            <MEAS_V>{{ meas_v }}</MEAS_V>
            <MEAS_I>{{ meas_i }}</MEAS_I>
            <MEAS_R>{{ meas_r }}</MEAS_R>
        </DATA>
    </DATA_SET>
</ROOT>
