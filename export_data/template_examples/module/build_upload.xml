<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ROOT xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<PARTS>
		<PART mode="auto">
			<KIND_OF_PART>{{ kind_of_part }}</KIND_OF_PART>
			<SERIAL_NUMBER>{{ ID }}</SERIAL_NUMBER>
			<BARCODE>{{ barcode }}</BARCODE>
			<LOCATION>{{ location }}</LOCATION>
			<INSTITUTION>{{ institution }}</INSTITUTION>
			<MANUFACTURER>{{ manufacturer }}</MANUFACTURER>
			<VERSION>{{ version }}</VERSION>
		<CHILDREN>
		<PART mode="auto">
			<KIND_OF_PART>{{ kind_of_part_protomodule }}</KIND_OF_PART>
			<SERIAL_NUMBER>{{ protomodule }}</SERIAL_NUMBER>
		</PART>
		<PART mode="auto">
			<KIND_OF_PART>{{ kind_of_part_pcb }}</KIND_OF_PART>
			<SERIAL_NUMBER>{{ pcb }}</SERIAL_NUMBER>
		</PART>
		</CHILDREN>
		</PART>
	</PARTS>
</ROOT>