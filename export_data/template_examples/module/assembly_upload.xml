<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ROOT xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<HEADER>
	<TYPE>
		<EXTENSION_TABLE_NAME>HGC_MOD_ASMBLY</EXTENSION_TABLE_NAME>
		<NAME>HGC Eight Inch Module Assembly</NAME>
	</TYPE>
	<RUN>
		<!-- <RUN_TYPE>Module Assembly</RUN_TYPE> -->
		<RUN_TYPE>Si module assembly and inspection</RUN_TYPE>
        <RUN_NUMBER>{{ run_number }}</RUN_NUMBER>
		<RUN_BEGIN_TIMESTAMP>{{ run_begin_timestamp_ }}</RUN_BEGIN_TIMESTAMP>
		<RUN_END_TIMESTAMP>{{ run_end_timestamp_ }}</RUN_END_TIMESTAMP>
		<INITIATED_BY_USER>{{ initiated_by_user }}</INITIATED_BY_USER>
		<LOCATION>{{ location }}</LOCATION>
		<COMMENT_DESCRIPTION>Module assembly</COMMENT_DESCRIPTION>
	</RUN>
</HEADER>
	<DATA_SET>
		<COMMENT_DESCRIPTION>Module Assembly</COMMENT_DESCRIPTION>
		<VERSION>1</VERSION>
		<PART>
			<KIND_OF_PART>{{ kind_of_part }}</KIND_OF_PART>
			<SERIAL_NUMBER>{{ ID }}</SERIAL_NUMBER>
		</PART>
		<DATA>
			<ASMBL_USER>{{ asmbl_user }}</ASMBL_USER>
			<ASMBL_TRAY_NAME>{{ asmbl_tray_name }}</ASMBL_TRAY_NAME>
			<PRTO_SER_NUM>{{ protomodule }}</PRTO_SER_NUM>
			<PRTO_ASM_ROW>{{ asm_row }}</PRTO_ASM_ROW>
			<PRTO_ASM_COL>{{ asm_col }}</PRTO_ASM_COL>
			<PCB_CMP_ROW>{{ asm_row }}</PCB_CMP_ROW>
			<PCB_CMP_COL>{{ asm_col }}</PCB_CMP_COL>
			<PCB_TOOL_NAME>{{ pcb_tool_name }}</PCB_TOOL_NAME>
			<GLUE_TYPE>{{ glue_type }}</GLUE_TYPE>
			<GLUE_BATCH_NUM>{{ glue_batch_num }}</GLUE_BATCH_NUM>
			<COMP_TRAY_NAME>{{ comp_tray_name }}</COMP_TRAY_NAME>
			<TRANSFER_TAPE_BATCH_NUM>{{ transfer_tape_batch_num }}</TRANSFER_TAPE_BATCH_NUM>
		</DATA>
	</DATA_SET>
</ROOT>