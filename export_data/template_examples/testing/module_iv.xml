<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ROOT xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<HEADER>
    <TYPE>
        <EXTENSION_TABLE_NAME>SI_MODULE_IV_TEST</EXTENSION_TABLE_NAME>
        <NAME>Si Module IV Test Data</NAME>
    </TYPE>
    <RUN>
        <!-- <RUN_NAME>Current/voltage measurements</RUN_NAME> -->
        <RUN_TYPE>Si module current-voltage test</RUN_TYPE>
        <RUN_BEGIN_TIMESTAMP>{{ run_begin_timestamp_ }}</RUN_BEGIN_TIMESTAMP>
        <RUN_END_TIMESTAMP>{{ run_end_timestamp_ }}</RUN_END_TIMESTAMP>
		<RUN_NUMBER>{{ run_number }}</RUN_NUMBER>
        <INITIATED_BY_USER>{{ initiated_by_user }}</INITIATED_BY_USER>
        <LOCATION>{{ location }}</LOCATION>
        <COMMENT_DESCRIPTION>Measured IV test data</COMMENT_DESCRIPTION>
    </RUN>

</HEADER>

    <DATA_SET>
        <COMMENT_DESCRIPTION>Upload measured data</COMMENT_DESCRIPTION>
        <VERSION>1</VERSION>
        <PART mode="auto">
            <SERIAL_NUMBER>{{ ID }}</SERIAL_NUMBER>
            <KIND_OF_PART>{{ kind_of_part }}</KIND_OF_PART>
        </PART>
        <DATA>
            <!-- This goes to the env table -->
            <!-- <REL_HUM>{{ rel_hum }}</REL_HUM> -->
            <!-- <TEMP_C>{{ temp_c }}</TEMP_C> -->
            
            <!-- This goes to the json file -->
            <!-- <PROG_V>{{ prog_v }}</PROG_V>
            <MEAS_V>{{ meas_v }}</MEAS_V>
            <MEAS_I>{{ meas_i }}</MEAS_I>
            <MEAS_R>{{ meas_r }}</MEAS_R> -->
            <TESTER>{{ tester }}</TESTER>
            <STATUS>{{ status }}</STATUS>
            <STATUS_DESC>{{ status_desc }}</STATUS_DESC>
            <REF_VOLT_A>{{ ref_volt_a }}</REF_VOLT_A>
            <REF_VOLT_B>{{ ref_volt_b }}</REF_VOLT_B>
            <CURRENT_AMPS_AT_VOLT_A>{{ current_amps_at_volt_a }}</CURRENT_AMPS_AT_VOLT_A>
            <CURRENT_RATIO_B_OVER_A>{{ current_ratio_b_over_a }}</CURRENT_RATIO_B_OVER_A>
            <DATA_POINT_COUNT>{{ data_point_count }}</DATA_POINT_COUNT>
            <DATA_POINTS_JSON>{{ data_points_json }}</DATA_POINTS_JSON>
            <GRADE>{{ grade }}</GRADE>
            <COMMENTS>{{ comments }}</COMMENTS>
        </DATA>
    </DATA_SET>
</ROOT>