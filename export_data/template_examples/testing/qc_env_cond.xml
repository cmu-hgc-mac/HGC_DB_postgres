<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ROOT xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<HEADER>
    <TYPE>
        <EXTENSION_TABLE_NAME>HGC_COND_TEMP_HUMID</EXTENSION_TABLE_NAME>
        <NAME>Environmental conditions</NAME>
    </TYPE>
    <RUN>
        <RUN_TYPE>MAC Environmental Condition</RUN_TYPE>
        <RUN_NUMBER>{{ run_number }}</RUN_NUMBER>
        <RUN_BEGIN_TIMESTAMP>{{ run_begin_timestamp_ }}</RUN_BEGIN_TIMESTAMP>
        <RUN_END_TIMESTAMP>{{ run_end_timestamp_ }}</RUN_END_TIMESTAMP>
        <INITIATED_BY_USER>{{ initiated_by_user }}</INITIATED_BY_USER>
        <LOCATION>{{ location }}</LOCATION>
        <COMMENT_DESCRIPTION>Measured environmental data</COMMENT_DESCRIPTION>
    </RUN>
</HEADER>
    <DATA_SET>
        <COMMENT_DESCRIPTION>Temp and humidity measurements</COMMENT_DESCRIPTION>
        <VERSION>1</VERSION>
        <PART mode="auto">
            <KIND_OF_PART>{{ kind_of_part }}</KIND_OF_PART>
            <SERIAL_NUMBER>{{ ID }}</SERIAL_NUMBER>
        </PART>
        <DATA>
            <MEASUREMENT_TIME>{{ run_begin_timestamp_ }}</MEASUREMENT_TIME>
            <TEMP_C>{{ temp_c }}</TEMP_C>
            <HUMIDITY_REL>{{ humidity_rel }}</HUMIDITY_REL>
            <TEMPSENSOR_ID>{{ tempsensor_id }}</TEMPSENSOR_ID>
        </DATA>
    </DATA_SET>
</ROOT>