<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ROOT xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<HEADER>
    <TYPE>
        <EXTENSION_TABLE_NAME>SI_MODULE_PEDESTAL_TEST</EXTENSION_TABLE_NAME>
        <NAME>Si Module Pedestal Test Data</NAME>
    </TYPE>
    <RUN>
        <RUN_NAME>Current/voltage measurements</RUN_NAME>
        <!-- <RUN_BEGIN_TIMESTAMP>{{ run_begin_timestamp_ }}</RUN_BEGIN_TIMESTAMP>
        <RUN_END_TIMESTAMP>{{ run_end_timestamp_ }}</RUN_END_TIMESTAMP> -->
        <INSPECTOR>{{ inspector }}</INSPECTOR>
        <!-- <LOCATION>{{ location }}</LOCATION> -->
        <COMMENT_DESCRIPTION>Measured pedestal test data</COMMENT_DESCRIPTION>
    </RUN>

</HEADER>

    <DATA_SET>
        <COMMENT_DESCRIPTION>Upload measured data</COMMENT_DESCRIPTION>
        <VERSION>1</VERSION>
        <PART mode="auto">
            <SERIAL_NUMBER>{{ ID }}</SERIAL_NUMBER>
            <!-- <KIND_OF_PART>{{ kind_of_part }}</KIND_OF_PART> -->
        </PART>
        <DATA>
            <ADC_MEAN_HEXMAP>{{ adc_mean_hexmap }}</ADC_MEAN_HEXMAP>
            <ADC_STDD_HEXMAP>{{ adc_stdd_hexmap }}</ADC_STDD_HEXMAP>
            <NOISE_CHANNEL_CHIP>{{ noise_channel_chip }}</NOISE_CHANNEL_CHIP>
            <PEDESTAL_CHANNEL_CHIP>{{ pedestal_channel_chip }}</PEDESTAL_CHANNEL_CHIP>
            <TOTAL_CHANNEL_CHIP>{{ total_channel_chip }}</TOTAL_CHANNEL_CHIP>
            <COMMENT_PLOT_TEXT>{{ comment_plot_text }}</COMMENT_PLOT_TEXT>
        </DATA>
    </DATA_SET>
</ROOT>