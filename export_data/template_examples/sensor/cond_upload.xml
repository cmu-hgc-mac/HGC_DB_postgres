<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ROOT xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<HEADER>
    <TYPE>
        <EXTENSION_TABLE_NAME>HGC_SENSOR_IV_SUMRY</EXTENSION_TABLE_NAME>
		<NAME>HGC Sensor IV Summary</NAME>
    </TYPE>
    <RUN>
        <!-- <RUN_NAME>Flatness measurement</RUN_NAME> -->
        <RUN_TYPE>MAC sensor visual inspection</RUN_TYPE>
        <RUN_NUMBER>{{ run_number }}</RUN_NUMBER>
        <RUN_BEGIN_TIMESTAMP>{{ run_begin_timestamp_ }}</RUN_BEGIN_TIMESTAMP>
        <RUN_END_TIMESTAMP>{{ run_end_timestamp_ }}</RUN_END_TIMESTAMP>
        <INITIATED_BY_USER>{{ initiated_by_user }}</INITIATED_BY_USER>
        <LOCATION>{{ location }}</LOCATION>
        <COMMENT_DESCRIPTION>Upload MAC sensor visual inspection grade</COMMENT_DESCRIPTION>
    </RUN>

</HEADER>

    <DATA_SET>
        <COMMENT_DESCRIPTION>{{ comment_description }}</COMMENT_DESCRIPTION>
        <VERSION>1</VERSION>
        <PART mode="auto">
            <SERIAL_NUMBER>{{ ID }}</SERIAL_NUMBER>
            <KIND_OF_PART>{{ kind_of_part }}</KIND_OF_PART>
        </PART>

        <DATA>
			<MAC_GRADE>{{ mac_grade }}</MAC_GRADE>
        </DATA>
    </DATA_SET>
</ROOT>